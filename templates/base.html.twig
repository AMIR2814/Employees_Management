<!DOCTYPE html>
<html lang="fa" dir="rtl">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="fontiran.com:license" content="Y68A9">
    <link rel="icon" href="{{ asset('build/images/favicon.ico') }}" type="image/ico"/>
    <title>{% block title %}{% endblock %}</title>

    <!-- Bootstrap -->
    <link href="{{ asset('build/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('build/bootstrap-rtl/dist/css/bootstrap-rtl.min.css') }}" rel="stylesheet">

    <!-- Font Awesome -->
    <link href="{{ asset('build/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
    <!-- NProgress -->
    <link href="{{ asset('build/nprogress/nprogress.css') }}" rel="stylesheet">
    <!-- bootstrap-progressbar -->
    <link href="{{ asset('build/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css') }}" rel="stylesheet">
    <!-- iCheck -->
    <link href="{{ asset('build/iCheck/skins/flat/green.css') }}" rel="stylesheet">
    <!-- bootstrap-daterangepicker -->
    <link href="{{ asset('build/bootstrap-daterangepicker/daterangepicker.css') }}" rel="stylesheet">

    {% block style %}{% endblock %}
    <!-- Custom Theme Style -->
    <link href="{{ asset('build/css/custom.min.css') }}" rel="stylesheet">
</head>

<!-- /header content -->
<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <div class="col-md-3 left_col hidden-print">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <a href="#" class="site_title">
                        <img src="{{ asset('build/images/favicon.ico') }}" style="width:40px; height: 40px">
                        <span>{{ webSiteName }}</span>
                    </a>
                </div>
                <div class="clearfix"></div>

                <!-- menu profile quick info -->
                <div class="profile clearfix">
                    <div class="profile_pic">
                        <img src="{{ asset('build/images/img.jpg') }}" alt="..." class="img-circle profile_img">
                    </div>
                    <div class="profile_info">
                        <span>خوش آمدید,</span>
                        <h2>مرتضی کریمی</h2>
                    </div>
                </div>
                <!-- /menu profile quick info -->

                <br/>

                <!-- sidebar menu -->
                {% include 'page-elements/sidebarMenu.html.twig' %}
                <!-- /sidebar menu -->

            </div>
        </div>

        <!-- top navigation -->
        {% include 'page-elements/topNavigation.html.twig' %}
        <!-- /top navigation -->
<!-- /header content -->

        <!-- page content -->
        <div class="right_col" role="main">
            <div class="">

                <div class="page-title">
                    <div class="title_left">
                        <h3>{{ block('title') }}</h3>
                    </div>

                    <div class="title_right">

                        {% block serachbox %}{% endblock %}

                    </div>
                </div>

                <div class="clearfix"></div>

                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <h2>

                                    {% block pageDescription %}{% endblock %}

                                </h2>
                                <ul class="nav navbar-right panel_toolbox">
                                    <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                                    <li><a class="close-link"><i class="fa fa-close"></i></a></li>
                                </ul>
                                <div class="clearfix"></div>
                            </div>
                            <div class="x_content">

                                {% block body %}{% endblock %}

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /page content -->

        <!-- footer content -->
        {% include 'page-elements/footer.html.twig' %}
        <!-- /footer content -->
    </div>
</div>


<div id="lock_screen">
    <table>
        <tr>
            <td>
                <div class="clock"></div>
                <span class="unlock">
                    <span class="fa-stack fa-5x">
                      <i class="fa fa-square-o fa-stack-2x fa-inverse"></i>
                      <i id="icon_lock" class="fa fa-lock fa-stack-1x fa-inverse"></i>
                    </span>
                </span>
            </td>
        </tr>
    </table>
</div>

<!-- jQuery -->
<script src="{{ asset('build/jquery/dist/jquery.min.js') }}"></script>
<!-- Bootstrap -->
<script src="{{ asset('build/bootstrap/dist/js/bootstrap.min.js') }}"></script>
<!-- FastClick -->
<script src="{{ asset('build/fastclick/lib/fastclick.js') }}"></script>
<!-- NProgress -->
<script src="{{ asset('build/nprogress/nprogress.js') }}"></script>
<!-- bootstrap-progressbar -->
<script src="{{ asset('build/bootstrap-progressbar/bootstrap-progressbar.min.js') }}"></script>
<!-- iCheck -->
<script src="{{ asset('build/iCheck/icheck.min.js') }}"></script>

<!-- bootstrap-daterangepicker -->
<script src="{{ asset('build/moment/min/moment.min.js') }}"></script>

<script src="{{ asset('build/bootstrap-daterangepicker/daterangepicker.js') }}"></script>

<!-- Chart.js -->
<script src="{{ asset('build/Chart.js/dist/Chart.min.js') }}"></script>
<!-- jQuery Sparklines -->
<script src="{{ asset('build/jquery-sparkline/dist/jquery.sparkline.min.js') }}"></script>
<!-- gauge.js -->
<script src="{{ asset('build/gauge.js/dist/gauge.min.js') }}"></script>
<!-- Skycons -->
<script src="{{ asset('build/skycons/skycons.js') }}"></script>
<!-- Flot -->
<script src="{{ asset('build/Flot/jquery.flot.js') }}"></script>
<script src="{{ asset('build/Flot/jquery.flot.pie.js') }}"></script>
<script src="{{ asset('build/Flot/jquery.flot.time.js') }}"></script>
<script src="{{ asset('build/Flot/jquery.flot.stack.js') }}"></script>
<script src="{{ asset('build/Flot/jquery.flot.resize.js') }}"></script>
<!-- Flot plugins -->
<script src="{{ asset('build/flot.orderbars/js/jquery.flot.orderBars.js') }}"></script>
<script src="{{ asset('build/flot-spline/js/jquery.flot.spline.min.js') }}"></script>
<script src="{{ asset('build/flot.curvedlines/curvedLines.js') }}"></script>
<!-- DateJS -->
<script src="{{ asset('build/DateJS/build/production/date.min.js') }}"></script>
<!-- JQVMap -->
<script src="{{ asset('build/jqvmap/dist/jquery.vmap.js') }}"></script>
<script src="{{ asset('build/jqvmap/dist/maps/jquery.vmap.world.js') }}"></script>
<script src="{{ asset('build/jqvmap/examples/js/jquery.vmap.sampledata.js') }}"></script>
<!-- morris.js -->
<script src="{{ asset('build/raphael/raphael.min.js') }}"></script>
<script src="{{ asset('build/morris.js/morris.min.js') }}"></script>

<!-- Custom Theme Scripts -->
<script src="{{ asset('build/js/custom.min.js') }}"></script>

{% block javaScript %}{% endblock %}

</body>
</html>
